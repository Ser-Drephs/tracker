{
  "Serilog": {
    "Using": [ "Serilog.Sinks.File"],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "File",
              "Args": {
                "path": "%temp%/TrackerLogs/tracker.log",
                "rollingInterval": "Day"
              }
            }
          ]
        }
      }
    ]
  }
}
